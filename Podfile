source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
inhibit_all_warnings!
use_frameworks!


def application_pods
    pod 'SnapKit', '~> 3.0'
    pod 'SwiftyJSON', '3.1.4'
    pod 'Swinject', '2.0.0'
    pod 'SwinjectAutoregistration', '2.0.1'
    pod 'Alamofire', '4.4.0'
end

target 'ChallengeAccepted' do
    application_pods
end

target 'ChallengeAcceptedTests' do
    application_pods
end
